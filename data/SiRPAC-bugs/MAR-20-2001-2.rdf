<?xml version="1.0"?>

<!--
    If a propertElt has a parseType="Resource" or parseType="Literal" 
    attribute then the only attribute that is permitted is ID. If other 
    attributes are included, SiRPAC does not raise an error
  -->

<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:a="http://example.org/a#">

<rdf:Description about="res_1">
  <a:c rdf:parseType="Resource" rdf:about="val">
    <a:d>text</a:d>
  </a:c> 
</rdf:Description>

<rdf:Description about="res_2">
  <a:c rdf:parseType="Literal" rdf:about="val">
    <a:d>text</a:d>
  </a:c> 
</rdf:Description>

</rdf:RDF>
